<mapper namespace="cn.stylefeng.guns.modular.system.dao.HouseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.system.model.House">
        <id column="id" property="id" />
